    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "typescript": "4.2.2"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  }
